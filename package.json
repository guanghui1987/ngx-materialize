{
    "name": "ngx-materialize",
    "description": "An Angular wrap around Materialize library",
    "license": "Apache-2.0",
    "repository": "sherweb/ngx-materialize",
    "keywords": [
        "angular",
        "ng",
        "ngx",
        "material",
        "material-design",
        "materialize",
        "materializecss",
        "component",
        "ui"
    ],
    "scripts": {
        "ng": "ng",
        "start": "yarn link:lib && ng serve",
        "build:demo": "ng build demo",
        "build:lib": "ng build lib",
        "postbuild:lib": "cpx ./README.md ./dist/lib",
        "link:lib": "rimraf node_modules/ngx-materialize && linklocal",
        "lint:ci": "ng lint lib --format checkstyle > tslint.xml",
        "lint:demo": "ng lint demo",
        "lint:lib": "ng lint lib",
        "test": "ng test",
        "postinstall": "typings install",
        "gh-pages:build": "yarn build:demo --prod --aot --base-href /ngx-materialize/",
        "gh-pages:deploy": "angular-cli-ghpages --dir=\"dist/demo\" --name=\"CircleCI\" --email=\"circleci@email.com\" --no-silent",
        "heroku-postbuild": "yarn install && yarn build:demo --prod --aot",
        "publish:lib": "npm publish ./dist/lib"
    },
    "dependencies": {
        "@angular/animations": "~7.2.0",
        "@angular/common": "~7.2.0",
        "@angular/compiler": "~7.2.0",
        "@angular/core": "~7.2.0",
        "@angular/forms": "~7.2.0",
        "@angular/http": "~7.2.0",
        "@angular/platform-browser": "~7.2.0",
        "@angular/platform-browser-dynamic": "~7.2.0",
        "@angular/router": "~7.2.0",
        "@mdi/font": "^2.4.85",
        "@types/jquery": "^3.3.29",
        "core-js": "^2.5.4",
        "highlightjs": "^9.10.0",
        "ngx-malihu-scrollbar": "^7.0.0",
        "ngx-markdown": "^1.6.0",
        "ngx-materialize": "file:./lib",
        "rxjs": "^6.3.3",
        "web-animations-js": "^2.3.1",
        "zone.js": "^0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.13.0",
        "@angular-devkit/build-ng-packagr": "~0.13.0",
        "@angular/cli": "~7.3.1",
        "@angular/compiler-cli": "~7.2.0",
        "@angular/language-service": "~7.2.0",
        "@types/jasmine": "~2.8.6",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~8.9.4",
        "angular-cli-ghpages": "^0.5.2",
        "codelyzer": "~4.2.1",
        "cpx": "^1.5.0",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~1.7.1",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~1.4.2",
        "karma-jasmine": "~1.1.1",
        "karma-jasmine-html-reporter": "^0.2.2",
        "karma-junit-reporter": "^1.2.0",
        "linklocal": "^2.8.2",
        "ng-packagr": "^3.0.0-rc.2",
        "rimraf": "^2.6.2",
        "ts-node": "~7.0.0",
        "tsickle": ">=0.25.5",
        "tslib": "^1.9.0",
        "tslint": "~5.9.1",
        "tslint-eslint-rules": "^5.2.0",
        "tslint-microsoft-contrib": "^5.0.3",
        "typescript": "^3.2.2",
        "typings": "^2.0.0"
    }
}
